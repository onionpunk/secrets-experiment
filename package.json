{
  "name": "root",
  "private": true,
  "scripts": {
    "clean": "lerna clean --yes && lerna run clean && rm -rf .cover/ .js/ .dist/",
    "build": "lerna run build",
    "start": "npm run clean && lerna bootstrap --ignore-scripts && npm run build",
    "start:ci": "lerna bootstrap --ci --ignore-scripts && npm run build",
    "test": "jest --coverage=true --no-cache --maxWorkers=2 --ci \"/tests/.*\\.(test|spec)?\\.(ts|tsx)$\"",
    "test:ci": "jest --config=jest.config.ci.js --coverage=true --no-cache --maxWorkers=2",
    "test:watch": "npm run test -- --watch --projects",
    "deploy:np": "lerna run --concurrency 3 --scope @cnf/master-catalog --scope @cnf/replicate-catalog-service --scope @cnf/compliance --scope @cnf/products --scope @cnf/reaper --scope @cnf/route53-resource deploy:np",
    "deploy:prod": "lerna run  --concurrency 3 --scope @cnf/master-catalog --scope @cnf/replicate-catalog-service --scope @cnf/compliance --scope @cnf/products --scope @cnf/reaper --scope @cnf/route53-resource deploy:prod",
    "sync": "lerna run --scope @cnf/products sync -- --",
    "synth": "lerna run --scope @cnf/products synth -- --",
    "verify": "lerna run verify --scope @cnf-products/*"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.4.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.14.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-modern-regexp": "0.0.6",
    "fork-ts-checker-webpack-plugin": "^0.5.0",
    "fs-extra": "^7.0.0",
    "jest": "^24.7.1",
    "lerna": "^3.13.3",
    "serverless": "^1.39.1",
    "serverless-pseudo-parameters": "^2.2.0",
    "serverless-step-functions": "^1.17.0",
    "serverless-webpack": "^5.2.0",
    "simple-git": "^1.110.0",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.3",
    "webpack": "^4.30.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {}
}
